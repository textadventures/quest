  <function name="GetDefaultPrefix" type="string" parameters="obj">
    gender = obj.gender
        if (gender = "el") {
          return ("un")
        }
        else if (gender = "ea") {
          return ("o")
        }
        else {
          return ("niște")
        }
  </function>

  <function name="WriteVerb" type="string" parameters="obj, verb">
    return (CapFirst(Conjugate(obj, verb)))
  </function>

  <function name="Deschis" type="string" parameters="obj">
    gender = obj.gender
    if (gender = "el") {
      return ("deschis")
    }
    else if (gender = "ea") {
      return ("deschisă")
    }
    else if (gender = "ei") {
      return ("deschiși")
    }
    else if (gender = "ele") {
      return ("deschise")
    }
    else {
      return ("deschisă")
    }
  </function>

  <function name="Inchis" type="string" parameters="obj">
    gender = obj.gender
    if (gender = "el") {
      return ("închis")
    }
    else if (gender = "ea") {
      return ("închisă")
    }
    else if (gender = "ei") {
      return ("închiși")
    }
    else if (gender = "ele") {
      return ("închise")
    }
    else {
      return ("închisă")
    }
  </function>

  <function name="Plin" type="string" parameters="obj">
    gender = obj.gender
    if (gender = "el") {
      return ("plin")
    }
    else if (gender = "ea") {
      return ("plină")
    }
    else if (gender = "ei") {
      return ("plini")
    }
    else if (gender = "ele") {
      return ("pline")
    }
    else {
      return ("plin")
    }
  </function>

  
  <function name="Descuiat" type="string" parameters="obj">
    gender = obj.gender
    if (gender = "el") {
      return ("descuiat")
    }
    else if (gender = "ea") {
      return ("descuiată")
    }
    else if (gender = "ei") {
      return ("descuiați")
    }
