    <!-- Editor-specific templates - should eventually be moved to e.g. EditorEnglish.aslx -->
    <template name="LanguageSpecificObjectTypes"></template>
    <!-- NOTE: To add a language-specific object type, fill in the template like this: -->
    <!--><template name="LanguageSpecificObjectTypes">masculine=Inanimate object (masculine); feminine=Inanimate object (feminine); </template>-->
    <!-- and then add <type>s masculine and feminine to the language .aslx file. The final "; " is important, don't delete it! -->
	
	<template name="LanguageSpecificObjectTypes">masculine=Objeto inanimado (masculino); feminine=Objeto inanimado (femenino); </template>
  
	<type name="masculine">
	  <gender>lo</gender>
	  <article>el</article>
	</type>
	  
	<type name="feminine">
	  <gender>la</gender>
	  <article>la</article>
	</type>

    <template name="EditorVerbDefaultExpression">"No puedes #verb#" + object.gender + "."</template>

    <function name="GetDefaultPrefix" type="string" parameters="obj">
      return(GetPrefix(obj,"un"))
    </function>
	
	<function name="WriteVerb" type="string" parameters="obj, verb">
	  return(Conjugate(obj, verb))
    </function>
	
	<function name="Conjugate" type="string" parameters="obj, verb">
	gender = obj.gender
    if (gender = "lo" or gender = "la") {
      gender = "él"
    } else if (gender = "los" or gender = "las") {
      gender = "ellos"
	}
	
    switch (verb) {
      case ("ser") {
        switch (gender) {
          case ("yo") {
            return ("soy")
          }
          case ("tú") {
            return ("eres")
          }
          case ("él") {
            return ("es")
          }
          case ("nosotros") {
            return ("somos")
          }
          case ("vosotros") {
            return ("sois")
          }
		  case ("ellos") {
            return ("son")
          }
          default {
            return ("ser")
          }
        }
      }
	  case ("estar") {
        switch (gender) {
          case ("yo") {
            return ("estoy")
          }
          case ("tú") {
            return ("estás")
          }
          case ("él") {
            return ("está")
          }
          case ("nosotros") {
            return ("estamos")
          }
          case ("vosotros") {
            return ("estáis")
          }
		  case ("ellos") {
            return ("están")
          }
          default {
            return ("estar")
          }
        }
      }
      case ("hacer") {
        switch (gender) {
          case ("yo") {
            return ("hago")
          }
          case ("tú") {
            return ("haces")
          }
          case ("él") {
            return ("hace")
          }
          case ("nosotros") {
            return ("hacemos")
          }
          case ("vosotros") {
            return ("hacéis")
          }
		  case ("ellos") {
            return ("hacen")
          }
          default {
            return ("hacer")
          }       
        }
      }
	  case ("tener") {
        switch (gender) {
          case ("yo") {
            return ("tengo")
          }
          case ("tú") {
            return ("tienes")
          }
          case ("él") {
            return ("tienen")
          }
          case ("nosotros") {
            return ("tenemos")
          }
          case ("vosotros") {
            return ("tenéis")
          }
		  case ("ellos") {
            return ("tienen")
          }
          default {
            return ("tener")
          }       
        }
      }
      default {
		return (verb)
      }

    }
  </function>

    <function name="GetPrefix" type="string" parameters="obj,type">
		if ( DoesInherit (obj, "namedmale") or DoesInherit (obj, "namedfemale") ) {
            return ("a")
        } else if ( DoesInherit (obj, "male") or DoesInherit (obj, "masculine") ) {
            return ("un")
        } else if ( DoesInherit (obj, "female") or DoesInherit (obj, "feminine") ) {
            return ("una")
        } else if ( DoesInherit (obj, "plural") or  DoesInherit (obj, "maleplural") ) {
            return ("unos")
        } else if ( DoesInherit (obj, "plural") or DoesInherit (obj, "femaleplural") ) {
            return ("unas")
        } else { 
            return ("un") 
        }
    </function>  
	
	<function name="GetSuffix" type="string" parameters="obj">
		if ( DoesInherit (obj, "male") or DoesInherit (obj, "masculine") ) {
		  return ("o")
		} else if ( DoesInherit (obj, "female") or DoesInherit (obj, "feminine") ) {
		  return ("a")
		} else if ( DoesInherit (obj, "plural") and DoesInherit (obj, "femaleplural") ) {
		  return ("as")
		} else if ( DoesInherit (obj, "plural") or  DoesInherit (obj, "maleplural") ) {
		  return ("os")
		} else {
		  return ("o")
		}
    </function>

