        <attr name="feature_container" type="boolean">false</attr>
        <close type="boolean">false</close>
        <displayverbs type="stringlist">
          <value>Look at</value>
          <value>Attack</value>
        </displayverbs>
        <alias>Rust monster</alias>
        <desc>A beetle-like reptile, with two long tendrils extending from its mouth, and a spiky tail.</desc>
        <defence type="int">1</defence>
        <treasurechance type="int">5</treasurechance>
        <attackonsight />
        <attackbonus type="int">2</attackbonus>
        <damagedicenumber type="int">3</damagedicenumber>
        <damagedicesides type="int">8</damagedicesides>
        <damagebonus type="int">2</damagebonus>
        <onweaponhit type="script">
          if (player.equipped.canberusted) {
            msg ("Your " + player.equipped.alias + " has been rusted by its contact with the creature. It will not be so effective from now on (but it is immune to further rusting).")
            player.equipped.canberusted = false
            player.equipped.rusted = true
            player.equipped.attackbonus = player.equipped.attackbonus - 2
            player.equipped.damagebonus = player.equipped.damagebonus - 2
            player.equipped.damagedicesides = player.equipped.damagedicesides - 2
            player.equipped.alias = player.equipped.alias + " (rusted)"
          }
        </onweaponhit>
        <openscript type="script">
        </openscript>
      </object>
      <object name="amorphous_blob">
        <inherit name="editor_object" />
        <inherit name="monster" />
        <alias>Amorphous blob</alias>
        <level type="int">7</level>
        <attackonsight />
        <canberusted type="boolean">false</canberusted>
        <nocorpse />
