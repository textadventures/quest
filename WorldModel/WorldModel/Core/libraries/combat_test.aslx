        l = NewObjectList()
        foreach (obj, ScopeVisibleNotHeld ()) {
          if (DoesInherit(obj, "monster")) {
            if (DoesInherit(obj, "multi_monster")) {
              if (not obj.dead) {
                list add (l, obj)
              }
            }
          }
        }
        if (ListCount(l) = 0) {
          msg ("No suitable monsters present")
        }
        else {
          o = PickOneObj(l)
          msg ("You have charmed " + GetDisplayAlias(o) + ".")
          do (o, "settodefend")
        }
      </spelleffect>
